<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="" prefHeight="400.0" prefWidth="600.0" styleClass="form_container" stylesheets="@../css/styles.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.pi.TropheeController">
    <children>
        <VBox layoutX="14.0" layoutY="90.0" prefHeight="253.0" prefWidth="173.0">
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Nom" />
                <TextField fx:id="tNom" />
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Description" />
                <TextField fx:id="tDescription" />
            </children>
        </VBox>
        <TableView fx:id="tTrophee" layoutX="240.0" layoutY="90.0" prefHeight="244.0" prefWidth="335.0">
            <columns>
                <TableColumn fx:id="nom" prefWidth="115.0" text="Nom" />
                <TableColumn fx:id="description" prefWidth="219.0" text="Description" />
            </columns>
        </TableView>
        <Button fx:id="btnSave" layoutX="182.0" layoutY="348.0" mnemonicParsing="false" onAction="#createTrophee" text="Save" />
        <Button fx:id="btnUpdate" layoutX="286.0" layoutY="348.0" mnemonicParsing="false" onAction="#updateTrophee" text="Update" />
        <Button fx:id="btnDelete" layoutX="390.0" layoutY="348.0" mnemonicParsing="false" onAction="#deleteTrophee" text="Delete" />
        <Button fx:id="btnClear" layoutX="494.0" layoutY="348.0" mnemonicParsing="false" onAction="#clear" text="Clear" />
      <Label layoutX="220.0" layoutY="29.0" styleClass="title" text="Trophees" />
    </children>
</AnchorPane>
